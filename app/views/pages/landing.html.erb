<h1>Students</h1>
<br>

<br>


  <table class="table table-hover col-6">
    <thead>
      <tr>
        <th scope="col"><h3>Name</h3></th>
        <th scope="col"><h3>Email</h3></th>
      </tr>
    </thead>
  <% @students.each do |student| %>

    <tbody>
      <tr class="table-danger">
        <th><%= student.name %></th>
        <td><%= student.email %></td>
      </tr>
    </tbody>
  <% end %>
  </table>
